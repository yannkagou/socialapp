
{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/npm"
      },
      {
        "src": "backend/**",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/backend/app.asgi:application",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ]
  }